<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  
 <mapper namespace="finalpro.technical" >
 	<select id="codelist"  resultType="stockdinfo">
 		select * from stockcode 
 	</select>
 	
 	<select id="infolist"  parameterType="String" resultType="stockdinfo">
 		select c.*, i.* from stockcode c, stockdinfo i where c.code = i.code and i.code=#{code}
 	</select>
 </mapper>  